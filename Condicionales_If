#Cambiar el operador para que se cumpla la sentencia
#Ejecicio 1 condicionales
num1 = 15
num2 = 20
#if num1 == num2 Condicion pasada
if num1 <= num2:  

	print('Se ejecuta el if del primer ejercicio.')

#Ejecicio 2 condicionales
num1 = 1450
num2 = 60
# if num1 < num2: condicion pasada
if num1 > num2:
	print('Se ejecuta el if del segundo ejercicio.')

#Ejecicio 3 condicionales
num1 = 1450
num2 = 60

#if num1 != num2: condicion pasada
if num1 == num2:
	print('Se ejecuta el if del tercer ejercicio.')
